#include <iostream>
using namespace std ;
float moyenne(float a, float b) {
    return (a+b)/2 ;
}
int main(void) {
    float v1, v2 ;
    cout << "Donner deux rÃ©els." << endl ;
    cin >> v1 >> v2 ;
    cout << "La moyenne de " << v1 << " et " << v2 << " est " << moyenne(v1,v2) << "." ;
    return 0 ;
}

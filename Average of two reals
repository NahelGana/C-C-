#include <iostream>
using namespace std ;

float average(float a, float b) {
    return (a+b)/2 ;
}
int main(void) {
    float v1, v2 ;
    cout << "Give two reals !" << endl ;
    cin >> v1 >> v2 ;
    cout << "The average of " << v1 << " and of " << v2 << " is " << average(v1, v2) << "." ;
    return 0 ;
}

#include <math.h>
#include <stdlib.h>
#include <iostream>
using namespace std;

int main(void) {
    int n ;
    cout << "Donnez un entier !" ;
    cin >> n ;
    if (n % 5 == 0) {
        cout << n << " is a multiple of 5." ;
    }
    cout << endl ;
    if (n % 7 == 0) {
        cout << n << " is a multiple of 7." ;
    }
    else if (n % 5 != 0) {
        cout << n << " is not a multiple of 5 and is not a multiple of 7." ;
    }
    return 0 ;
}
